{
  "name": "@slideclaw/cli",
  "version": "0.1.0",
  "description": "CLI for slideClaw",
  "type": "module",
  "bin": {
    "slideclaw": "./dist/index.mjs"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --tsconfig tsconfig.json",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.mjs"
  },
  "dependencies": {
    "@slideclaw/core": "workspace:*",
    "commander": "^13.1.0",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "tsup": "^8.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
