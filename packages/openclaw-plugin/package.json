{
  "name": "@slideclaw/openclaw-plugin",
  "version": "0.1.0",
  "description": "OpenClaw plugin for slideClaw integration",
  "main": "src/index.ts",
  "openclaw": {
    "extensions": ["./src/index.ts"]
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.7.3"
  }
}
